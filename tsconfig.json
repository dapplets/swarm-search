{
    "compilerOptions": {
        "baseUrl": ".",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "emitDecoratorMetadata": false,
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "target": "es2019",
        "sourceMap": false,
        "lib": [
            "es2019", "dom"
        ],
        "types": [ "./typesPatch" ],
        "paths": {
            "google-adapter": ["packages/google-adapter/src"],
            "youtube-adapter": ["packages/youtube-adapter/src"],
            "search-dapplet": ["packages/search-dapplet/src"]
        }
    }
}